{"ast":null,"code":"import Card from '@/components/UI/Card.vue';\nimport items from '@/seeders/items';\nexport default {\n  name: 'HomeView',\n  components: {\n    Card\n  },\n  data: () => ({\n    items: items\n  })\n};","map":{"version":3,"names":["Card","items","name","components","data"],"sources":["/Users/romansmirnov/Desktop/vue3/Clash of clans/src/views/HomeView.vue"],"sourcesContent":["<template>\n  <div class=\"container\">\n    <div class=\"card__wrapper\" v-for=\"item in items\" :key=\"item.id\">\n      <Card :name=\"`${item.lvl} lvl`\" :title=\"item.title\" :body=\"item.descr\" :imgUrl=\"item.img\" :link=\"`/${item.alias}`\">\n        <template v-slot:body>\n          {{ item.descr }}\n        </template>\n        <template v-slot:footer>\n          <div class=\"card-stats\">\n            <div v-for=\"(stat, index) in item.info\" :key=\"index\" class=\"one-third\">\n              <div class=\"stat-value\">{{ stat.value }}</div>\n              <div class=\"stat-value\">{{ stat.title }}</div>\n            </div>\n          </div>\n        </template>\n      </Card>\n    </div>\n  </div>\n</template>\n\n<script>\nimport Card from '@/components/UI/Card.vue';\nimport items from '@/seeders/items';\nexport default {\n  name: 'HomeView',\n  components: {\n    Card\n  },\n  data: ()=>({\n    items: items\n  })\n};\n</script>\n\n<style lang=\"scss\" scoped>\n.card__wrapper{\n  margin-bottom: 15px;\n}\n</style>"],"mappings":"AAqBA,OAAOA,IAAG,MAAO,0BAA0B;AAC3C,OAAOC,KAAI,MAAO,iBAAiB;AACnC,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,UAAU,EAAE;IACVH;EACF,CAAC;EACDI,IAAI,EAAEA,CAAA,MAAK;IACTH,KAAK,EAAEA;EACT,CAAC;AACH,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}